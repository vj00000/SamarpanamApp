{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": false,
        "paths": {
            "@/*": [
                "./*"
            ],
            "tailwind.config": [
                "./tailwind.config.js"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "nativewind-env.d.ts"
    ]
}